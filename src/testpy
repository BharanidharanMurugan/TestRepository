_GHE_INSTANCE = os.environ['GHE_INSTANCE']


def write_to_file(filename, data):
    f = open(filename, "w")
    f.write(data)
    f.close()
    return filename
   
   
print(_GHE_INSTANCE)
write_to_file('test.txt', _GHE_INSTANCE)
    
    
